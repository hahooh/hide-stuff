{
    "manifest_version": 2,
    "name": "Hide Stuff",
    "version": "1.0.0",
    "version_name": "Alpaca beta",
    "description": "Hide stuff that you don't want to see",
    "content_scripts": [
        {
            "css": [
                "./content/style.css"
            ],
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "./content/content.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "./popup/popup.html"
    },
    "icons": {
        "16": "./icons/16x16.png",
        "48": "./icons/48x48.png",
        "128": "./icons/128x128.png"
    },
    "permissions": [
        "storage"
    ]
}